---
import BaseLayout from './BaseLayout.astro';
import TitleBlock from '../components/TitleBlock.astro';
const { content } = Astro.props;
---

<BaseLayout title="About" description="About me.">
  <TitleBlock title="About" subtitle="Get to know me a bit better!" />

  <article
    class="prose prose-headings:md dark:prose-invert prose-code:before:content-none prose-code:after:content-none prose-lead:"
  >
    <slot />
  </article>
</BaseLayout>
